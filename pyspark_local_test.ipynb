   "source": [
    "import pyspark\n",
    "from pyspark import SparkContext\n",
    "\n",
    "\n",
    "sc = SparkContext()\n",
    "\n",
    "nums = sc.parallelize([1, 2, 3, 4])\n",
    "nums.take(1)\n",
    "\n",
    "squared = nums.map(lambda x: x * x).collect()\n",
    "for num in squared:\n",
    "    print(\"%i \" % (num))\n"
   ]
